#include<stdio.h>
int main(){
int n,i,element;
int a[n];
printf("Enter number of elements you want to enter");
scanf("%d",&n);
for(i=0;i<n;i++)
scanf("%d",&a[i]);
printf("enter number to search");
scanf("%d",&element);
for(i=0;i<n;i++)
{
if(a[i]==element)
{
printf("Element found");
return 0;
}
}
printf("Not found");
return 0;
}
